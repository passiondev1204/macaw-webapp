body{
  -webkit-overflow-scrolling: touch;
  overflow: scroll;
}

body h5 a {
  text-decoration: none;
  color: white;
}
body h5 a:hover {
  color: #cecece;
}
body h5 span:after {
  color: yellow;
  content: '\f0e7';
  font-family: FontAwesome;
  font-weight: normal;
}
body .open-modal {
  height: 22px;
  width: 100%;
  display: block;
  border: solid white;
  font-family: Myriad Pro;
  cursor: pointer;
  outline: none;
  text-align: left;
  font-weight: normal;
  background-color: white;
  font-size: 16px;
  padding: 0px 0px 0px 0px;
  color: #333;
  position: relative;
  right: 2px;
}
body .modal-dialog {
  position: absolute;
  min-height: 100%;
  min-width: 100%;
}

body .modal-content-annotation {
  font-family: Myriad Pro;
  font-size: 14px;
  background-color: white;
  z-index: 2;
  will-change: transform;
  position: absolute;
  margin-left: -400px;
  width: 450px;
  height: auto;
  transform: scaleX(0);
  opacity: 0;
  border: 1px solid #cecece;
  width: 600px;
  transition: all 300ms cubic-bezier(0.72, 0.91, 0.35, 1.2);
  top: 0px;
}


body .modal-content-annotation .modal-header-annotation {
  font-family: Myriad Pro;
  min-height: 15px;
  padding: 20px 0px 0px 15px;
  border-bottom: 1px solid #cecece;
}

body .modal-content-annotation .modal-header-annotation .close-modal {
  position: absolute;
  padding: 10px;
  right: 0;
  top: -10px;
  cursor: pointer;
  font-size: 1.5em;
  transition: all 500ms;
  user-select: none;
}

body .modal-content-annotation .modal-header-annotation .close-modal:hover {
  transform: rotate(180deg);
}

body .modal-content-annotation .modal-body {
  height: 200px;
  padding: 15px;
  overflow: auto;
}

body .modal-content-annotation .modal-body-annotations {
  height: 250px;
  padding: 15px;
  overflow: auto;
  display: flex;
  justify-content: space-between;
}

body .modal-content-annotation .modal-footer {
  height: 60px;
  width: 100%;
  text-align: center;
  line-height: 4.5;
  border-top: 1px solid #cecece;
}

body .modal-content-annotation .close-modal-annotation{
  font-family: Myriad Pro;
  margin: 10px;
  height: 35px;
  width: 80px;
  color: #0081D3;
  border: 1px solid #0081D3 !important;
  background-color: #fff;
  border: none;
  outline: none;
  cursor: pointer;
  transition: 500ms;
